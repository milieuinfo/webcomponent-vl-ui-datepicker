{
  "name": "vl-ui-datepicker",
  "version": "1.1.0",
  "description": "De \"datepicker\" UI component van Webuniversum Vlaanderen als Web Component",
  "main": "index.js",
  "scripts": {
    "prepare": "npm update --dev vl-ui-util && npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "wct --config-file ./wct.local.conf.json",
    "test:e2e": "npm run test:server:start && npm run test:e2e:chrome && npm run test:e2e:firefox",
    "test:e2e:grid": "npm run test:server:start && npm run test:e2e:chrome:grid && npm run test:e2e:firefox:grid",
    "test:grid": "wct --config-file ./wct.docker.conf.json && npm run test:e2e:grid",
    "posttest:e2e": "pkill -f http-server",
    "test:e2e:chrome": "mocha --chrome ",
    "test:e2e:chrome:grid": "mocha  --chrome --grid",
    "test:e2e:firefox": "mocha --firefox",
    "test:e2e:firefox:grid": "mocha --firefox --grid",
    "test:e2e:opera": "mocha --opera",
    "test:e2e:safari": "mocha --safari",
    "test:server:start": "http-server &",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "minify *.src.js > vl-datepicker.js",
    "release:prepare:imports": "replace '../dist/datepicker.js' '/node_modules/vl-ui-datepicker/dist/datepicker.js' vl-datepicker.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-datepicker/style.css' vl-datepicker.js",
    "release:prepare:commit": "git add -f vl-datepicker.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "dependencies": {
    "vl-ui-button": "^3.0.0",
    "vl-ui-core": "^4.0.0",
    "vl-ui-icon": "^3.0.0",
    "vl-ui-input-field": "^2.0.0",
    "vl-ui-input-group": "^3.0.0"
  },
  "peerDependencies": {
    "vl-ui-core": "^3.0.0"
  },
  "devDependencies": {
    "@govflanders/vl-ui-datepicker": "3.9.1",
    "luxon": "^1.22.0",
    "vl-ui-modal": "^2.1.0",
    "vl-ui-select": "^3.0.1",
    "vl-ui-side-sheet": "^2.0.1",
    "vl-ui-util": "^4.0.1"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-datepicker.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-datepicker/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-datepicker#readme"
}
