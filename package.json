{
  "name": "vl-ui-datepicker",
  "version": "1.1.0",
  "description": "De \"datepicker\" UI component van Webuniversum Vlaanderen als Web Component",
  "main": "vl-datepicker.js",
  "scripts": {
    "prepare": "npm update --dev vl-ui-util && npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-datepicker.js -- *.src.js",
    "release:prepare:imports": "npm run release:prepare:imports:core && npm run release:prepare:imports:dist",
    "release:prepare:imports:core": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-datepicker.js",
    "release:prepare:imports:dist": "replace '../dist/datepicker.js' '/node_modules/vl-ui-datepicker/dist/datepicker.js' vl-datepicker.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-datepicker/style.css' vl-datepicker.js",
    "release:prepare:commit": "git add -f vl-datepicker.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "dependencies": {
    "vl-ui-button": "^1.2.0",
    "vl-ui-core": "^2.0.0",
    "vl-ui-icon": "^2.1.0",
    "vl-ui-input-field": "^1.0.12",
    "vl-ui-input-group": "^2.0.0"
  },
  "peerDependencies": {
    "vl-ui-core": "^2.0.0"
  },
  "devDependencies": {
    "@govflanders/vl-ui-datepicker": "3.9.1",
    "http-server": "0.12.0",
    "np": "3.1.0",
    "replace": "1.1.1",
    "sinon": "7.5.0",
    "uglify-js-es6": "2.8.9",
    "vl-ui-modal": "^1.0.11",
    "vl-ui-side-sheet": "^1.0.0",
    "vl-ui-util": "^2.0.10",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-datepicker.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-datepicker/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-datepicker#readme"
}
